@media only screen and (min-width : 320px) and (max-width : 568px)
  .content-page.pricing

    section.introduction
      padding-top           174px

      h2
        font-size         26px

      p
        font-size         18px
        line-height       24px

      .multiple-choice a
        width             43%

    section.packs
      h2
        font-size         18px

      .resource-pack
        width             100%
        margin-bottom     24px

        &:first-of-type:before
          display         none


    section.features
      h2
        font-size         18px

      .feature
        display             block
        width               auto
        padding             0 35px 44px 61px

        h5
          font-size         18px

        i
          vendor            transform, scale(.6)
          vendor            transform-origin, 0px 0px


