}

body.pricing

  #main-header .inner-container
    -webkit-font-smoothing  antialiased

.content-page.pricing
  width                 auto
  max-width             100%
  padding               0
  font-family           openSansFamily

  //Commons

  section
    h2.general-title
      font-size         22px
      font-weight       400
      text-align        center
      border-bottom     none
      margin            0
      padding           82px 0
      color             #656565

      span
        margin          0

    .inner-container
      max-width             1290px
      padding               0px 45px
      margin                0 auto
      display               block
      position              relative
      borderBox()

    .resource-pack
      border-right      1px solid #e9e9e9
      border-bottom     2px solid #f8f8f8
      border-top        2px solid #f8f8f8
      width             20%
      z-index           10
      borderBox()
      fl()

      &:first-of-type
        border-left     1px solid #e9e9e9
        position        relative
        overflow        visible

        &:before
          content       ""
          display       block
          size          500%, 2px
          abs           1px 0 0 -2px
          border-right  13px solid #f43500
          z-index       4
          bg            image, -webkit-linear-gradient(left, #1aaf5d 0%, #ffd400 50%, #f43500 100%) /,
                                  -moz-linear-gradient(left, #1aaf5d 0%, #ffd400 50%, #f43500 100%)

      h2.pack-title
        padding-left    75px
        height          78px
        line-height     78px
        border-bottom   2px solid #f8f8f8
        font-size       18px
        font-weight     600
        borderBox()

        span
          margin        0

        cite
          font-size     14px
          display       block
          abs           21px 0 0 29px
          size          32px, 32px
          rounded       18px
          border        2px solid #e9e9e9
          text-align    center
          line-height   32px
          background    #fff

      .pack-features
        padding         38px 8px 38px 31px
        border-bottom   2px solid #f8f8f8
        borderBox()

        dd
          font-size     18px
          line-height   28px
          color         #505050
          font-weight   400

          em
            font-weight 600

      .pack-buy-button
        background      #fff
        display         block
        kalc            width, 100% \- 8px
        margin          4px
        rounded         3px
        border          none
        outline         none
        shadow          none
        padding         23px 0
        font-size       18px
        color           #3e3e3e
        font-weight     400

        .button-title
          font-size     inherit
          color         inherit
          line-height   inherit
          font-weight   inherit
          position      relative
          display       inline-block
          text-decoration underline

          cite
            font-weight 600
            display     block
            abs         0 100% 0 0
            margin      0 10px 0 0
            noTextDeco()

        .icon
          r-sprite      "pricing", "buy-arrow"
          display       block
          margin        0
          padding       0
          abs           22px 22px 0 0


      &.blue //.resource-pack
        h2.pack-title
          color           #64d2c8
          cite
            border-color  #64d2c8

      &.green //.resource-pack
        h2.pack-title
          color           #5ecb66
          cite
            border-color  #5ecb66

      &.yellow //.resource-pack
        h2.pack-title
          color           #efc860
          cite
            border-color  #efc860

      &.orange //.resource-pack
        h2.pack-title
          color           #f4a360
          cite
            border-color  #f4a360

      &.red //.resource-pack
        h2.pack-title
          color           #f35f51
          cite
            border-color  #f35f51

      &:hover //.resource-pack
        background        url("/a/images/stripe.png")

        *:not(cite)
          border-color    transparent

  //\Commons

  section.introduction
    background          #3eac75
    padding             134px 0 85px
    text-align          center
    bg                  image, url("/a/images/pricing-pat.png")
    overflow            hidden

    &:after
      content           ""
      display           block
      size              100vw, 3px
      abs               0 0 0px 0
      bg                image, url("/a/images/wave-bg.png")
      bg                repeat, repeat-x

    h2
      border-bottom     none
      color             #fefefe
      font-size         34px
      font-weight       600
      line-height       38px
      height            auto
      opacity           .99
      overflow          visible

      span
        margin          0
        padding         0

    p
      color             #fefefe
      font-size         20px
      line-height       24px
      font-weight       300
      max-width         760px
      letter-spacing    .03em
      margin            28px auto 56px auto

    .multiple-choice
      font-family       openSansFamily
      rounded           0

      a
        size            188px, 43px
        line-height     43px
        letter-spacing  .05em
        background      transparent
        rounded         4px
        padding         0
        border          2px solid #89dfb4
        shadow          none
        text-shadow     none
        font-size       16px
        font-weight     400
        color           #f7fffb

        &:hover
          background    rgba(255, 255, 255, .2)

        &~a
          margin-left   14px

        &.active
          background    #f5f5f5
          border        2px solid #f5f5f5
          color         #5ca782

          &:hover
            opacity     .9


  .kdtabview.pricing-type-tab
    margin              0
    padding             0
    width               100%
    float               none

    .kdtabpaneview.developer
      padding-bottom    100px


  section.packs
    margin                0 auto
    max-width             1290px
    padding               0 45px
    width                 auto
    borderBox()


  section.team-plan
    margin                0 auto
    max-width             1290px
    padding               0 45px
    width                 auto
    borderBox()

    .custom-plan-container
      border              1px solid #e9e9e9
      position            relative

    .sliders-container
      kalc                width, 100% \- 278px
      padding             59px
      borderBox()
      fl()

    .resource-pack
      size                278px, 100%
      border              none
      border-left         1px solid #efefef
      borderBox()
      fr()

      .pack-buy-button
        .button-title
          cite
            margin-right  30px

    .plan-selection-box
      &:first-of-type
        margin-bottom     68px

      h4
        font-size         22px
        color             #6a6a6a
        font-weight       400
        display           block
        margin            0 0 40px 0

      .sliderbar-container
        size              100%, 3px
        border            none
        bg                image, -webkit-linear-gradient(left, #00c052 0%, #ffd800 50%, #fe0000 100%) /,
                          -moz-linear-gradient(left, #00c052 0%, #ffd800 50%, #fe0000 100%)

        .opposite
          background      #cecece
          border          none
          shadow          inset 0 2px 3px rgba(0, 0, 0, .14)


        .handle
          size            33px, 33px
          rounded         50%
          background      #fff
          border          1px solid #e0e0e0
          shadow          0 3px 5px rgba(0, 0, 0, .13)
          top             -16px
          margin-left     -16px

          &:after
            display       none

          &:before
            display       none

        .sliderbar-label
          color           transparent
          top             -9px

          &:after
            size          3px, 7px
            background    #e0e0e0

  section.custom-quote
    z-index               5
    text-align            center
    padding-bottom        128px

    h2.general-title
      clear               both
      padding             128px 0 36px

    .kdbutton.border-only
      margin              0 auto
      border              2px solid #00d067
      background          transparent
      rounded             4px

      .button-title
        color             #01ae57
        font-weight       400
        font-size         16px


  section.features
    background            #f8f8f8

    .inner-container
      margin              0 auto
      max-width           1290px
      padding             0 45px
      width               auto
      letter-spacing      0
      word-spacing        0
      borderBox()

    .feature
      letter-spacing      normal
      word-spacing        normal
      size                49%
      padding             0 35px 74px 108px
      position            relative
      display             inline-block
      vertical-align      top
      borderBox()

      h5
        font-size         24px
        font-weight       400
        color             #2fbd72
        display           block
        margin-bottom     10px

      p
        font-size         14px
        color             #505050
        line-height       21px
        font-weight       400

      p.description
        font-size         12px
        color             #a0a0a0
        line-height       16px
        margin-top        18px

      .icon
        display           block
        abs               0px 0 0 0px

        &.cpu
          r-sprite       "pricing", "cpu-icon"

        &.ram
          r-sprite       "pricing", "ram-icon"

        &.disk
          r-sprite       "pricing", "disk-icon"

        &.always-on
          r-sprite       "pricing", "always-on-icon"



  // .payment-workflow
  //   min-height            600px

  //   .kdview.kdloader
  //     top                 160px
  //     left                50%
  //     margin              0 0 0 -20px
  //     z-index             10
  //     abs()

  //   .kdbutton .kdloader
  //     margin              0

  h3.pricing-title
    text-align          center
    display             block
    font-size           28px
    font-weight         200
    color               #666666
    margin              56px 0 8px

  h6.pricing-subtitle
    text-align          center
    display             block
    font-size           18px
    font-weight         400
    color               #666666
    opacity             .5
    margin-bottom       55px


.pricing .product-form
  size                  100%, auto
  min-height            100vh
  padding               0
  borderBox()

  h1
    font-size       32px

  h2
    font-size       28px

  h3
    font-size       24px

  h1
  h2
  h3
    font-weight     200
    color           #1c2021
    margin-bottom   8px

  .inner-container
    max-width           1096px
    margin              0 auto

    >header
      padding-bottom    41px
      border-bottom     1px solid #d4d4d4

      p
        font-size       20px
        color           #757575
        font-weight     200
        line-height     26px
        width           702px
        fl()

      a
        color           #3c4752

      .pricing-toggle
        font-family     inherit
        rounded         12px
        background      emphasized
        size            302px, 59px
        shadow          inset 0 0 13px rgba(0, 0, 0, .36)
        fr()

        a
          background    transparent
          border        none
          shadow        none
          height        55px
          kalc          width, 50% \- 2px
          margin        2px 2px 0 0
          padding       0
          line-height   55px
          text-shadow   none
          color         #fff
          font-size     16px
          font-weight   200

          &:first-child
            margin      2px 0 0 2px

        a.active
          background    #27ae60
          color         emphasized
          rounded       10px
          font-weight   400


    .plan-selection
      .developer-plan
        height          300px
        overflow        visible

        &.initial
          opacity       0

    .plan-selection-box
      width           715px
      padding-top     42px
      position        relative
      fl()

      &:nth-of-type(2)
        margin-left   70px

      h4
        font-size     28px
        color         #1c2021
        font-weight   200
        line-height   34px
        display       inline-block
        position      relative

        cite
          display     block
          abs         -4px -53px 0 0
          size        43px, 43px
          line-height 43px
          text-align  center
          font-size   18px
          background  emphasized
          rounded     50%
          color       #fff

      h5
        font-size     22px
        font-weight   400
        color         #757575
        margin        4px 0 0

      .sliderbar-container
        width         100% !important
        background    #dadada
        shadow        inset 1px 1px 0 rgba(0, 0, 0, .16)
        height        7px
        border        none
        margin        67px auto 0

        .handle
          size        26px, 26px
          shadow      none
          border      none
          rounded     50%
          background  #00ba4e
          shadow      inset 0 -1px 2px 1px rgba(0, 0, 0, .1) \,
                      inset 0 -8px 17px #00ba4e \,
                      inset 0  0 0 1px #00ba4e \,
                      inset 0 1px 0 2px rgba(255, 255, 255, .3)
          top         -10px
          margin      0 0 0 -12px

          &:after
          &:before
            hidden()

        .sliderbar-label
          color           transparent
          margin-top      -45px
          margin-bottom   0

          &:after
            size          2px, 14px
            background    #bababa

      .description
        display           block
        font-size         12px
        font-weight       400
        color             #727575
        line-height       23px
        margin            20px 0 0
        span
          color           #999
        cite
          bg              color, #d3d3d3
          rounded         12px
          font-size       10px
          display         inline-block
          min-width       33px
          height          23px
          line-height     23px
          text-align      center
          padding         0 5px
          margin          0 5px 0 15px
          borderBox()

      a.pricing-show-details
        font-size         12px
        font-weight       200
        color             #747474
        abs               174px -30px 0 0

      &.selected
        margin              0
        padding             40px 0 0 50px
        width               275px
        border-left         1px solid #d4d4d4
        fr()

        h5
          font-size         24px
          color             #27ae60
          margin            16px 0
          cite
            font-size       18px
            font-style      italic
            font-weight     200

        .description
          min-height        18px
          margin            0
          color             rgba(242, 242, 242, .3)

        .kdbutton.solid.buy-now
          size              126px, 48px
          line-height       48px
          font-size         18px
          color             #056233
          padding           0
          rounded           6px
          font-weight       600
          margin            29px 0 0
          text-align        center
          text-transform    uppercase
          fl()

          .button-title
            line-height     inherit
            color           inherit
            font-size       inherit
            font-weight     inherit

    .developer-plan

      .sliderbar-container
        shadow                inset 1px 1px 0 rgba(0, 0, 0, .16) !important
        background            -webkit-linear-gradient(left, #00c052 0%, #ffd800 50%, #fe0000 100%) !important

        .opposite.bar
          background          #dadada
          shadow              inset 1px 1px 0 rgba(0, 0, 0, .16) \,
                              0 0 0 1px #dadada

      .pricing-show-details
          abs                 174px -30px 0 0

  .how-it-works
    bg                        color, #f1f1f1
    border                    1px solid #d3d3d3
    border-radius             12px
    max-width                 913px
    padding                   24px
    color                     #747474
    vendor                    column-count, 3
    vendor                    column-gap, 24px
    position                  relative
    margin-top                -88px
    borderBox()

    &:after
      content                 ""
      display                 block
      border                  11px solid transparent
      border-bottom-color     #f1f1f1
      abs                     -22px 192px 0 0

    &:before
      content                 ""
      display                 block
      border                  11px solid transparent
      border-bottom-color     #d3d3d3
      abs                     -23px 192px 0 0

    &.team
      margin-top              0

      &:before
      &:after
        right                 612px

    .item
      font-size               12px
      font-weight             200

      &:not(.cpu)
        vendor                column-break-inside, avoid

      h4
        font-size             18px
        font-weight           400
        margin                0 0 24px

      p
        margin-bottom         24px
        line-height           1.2em

.pricing-horizontal-divider
  size                656px, 1px
  background          -moz-linear-gradient(left, rgba(0,0,0,0) 0%, rgb(207,207,207) 50%, rgba(0,0,0,0) 100%)
  background          -webkit-linear-gradient(left, rgba(0,0,0,0) 0%,rgb(207,207,207) 50%,rgba(0,0,0,0) 100%)
  display             block
  margin              13px auto
  position            relative

  &:after
    content           ""
    size              13px, 13px
    abs               50% 0 0 50%
    background        #cfcfcf
    rounded           50%
    display           block
    vendor            transform, translateX(-50%) translateY(-50%)

.pricing-breadcrumb
  size                100%, 480px
  background          #323232
  abs                 0px 0 0 0px

  ul
    display           block
    size              100%, auto
    text-align        center
    color             #7f7f80
    padding-top       200px

    li
      display         inline-block
      margin          0 16px
      opacity         .6
      position        relative

      &:after
        content       ''
        display       block
        rounded       50%
        size          6px, 6px
        background    #7f7f80
        abs           -32px 0 0 50%
        margin-left   -3px

      &.active
        color         #fff
        opacity       1

        &:after
          background  #fff

        & ~ li
          opacity     1

  section
    display           none

.pricing-breadcrumb:not(.hidden) ~ .payment-workflow
  border-top          1px solid transparent

  .kdbutton.solid // Move these to fw [BC]
    rounded           3px

    &.green
      background      #61C17F

    &.red
      background       #D75B5B

    &.medium
      height           40px
      line-height      42px

  .formline
    padding            0 17px 0 0
    margin             0 0 16px
    borderBox()

    .input-wrapper
      margin           0

    &.button-field
      clear            both
      border           none
      margin-top       6px

      .kdbutton
        margin-left    6px

  .kdlabel
    font-size            14px
    color                #8D8D8D
    float                none
    line-height          19px
    margin               0 0 7px
    font-weight          200

  .kdinput.text
    rounded              3px
    background           transparent
    border               1px solid #C9C9C9
    padding              8px 13px 10px
    font-size            14px
    line-height          19px
    color                #4C4C4C
    font-weight          600
    opacity              .99
    margin               0

  .placeholder-helper
    top                  12px

    &.out
      top                -15px

  h3.pricing-title
    font-size            16px
    color                #222
    line-height          21px
    margin               0 0 24px
    font-weight          400

  section
    background           #fff
    width                auto
    margin               250px auto 0
    padding              36px
    rounded              3px

    &.pricing-sign-in
      max-width          330px

      .kdbutton
        width            100%
        margin-bottom    16px

