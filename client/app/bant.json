{
  "name": "app",
  "main": "./lib/index.coffee",
  "routes": {
    "": "home",
    "/": "home",
    "/R/:username": "referrer",
    "/:name?/Login": "login",
    "/:name?/Register": "register",
    "/:name?/Logout": "logout",
    "/:slug/:name": "members",
    "/member/:username": "member",
    "/:name?/Unsubscribe/:token/:email/:opt?": "unsubscribe",
    "/:name": "name"
  },
  "sprites": [
    "./lib/sprites"
  ],
  "styles": [
    "./lib/styl/*.styl",
    "./lib/styl/reactivity/*.styl",
    "./lib/styl/terminal/*.styl",
    "./lib/components/**/styl/*.styl"
  ]
}
