{
  "name": "app",
  "main": "./lib/index.coffee",
  "globals": {
    "COMPUTECONTROLLER_TIMEOUT": 30000
  },
  "routes": {
    "": "home",
    "/": "home",
    "/R/:username": "referrer",
    "/:name?/Login": "login",
    "/:name?/Logout": "logout",
    "/:slug/:name": "members",
    "/member/:username": "member",
    "/:name?/Unsubscribe/:token/:email/:opt?": "unsubscribe",
    "/:name": "name"
  }
}
