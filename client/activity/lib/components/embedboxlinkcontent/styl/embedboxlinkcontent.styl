.EmbedBoxLinkContent
  width                     100%
  height                    100px
  padding                   0 10px
  overflow                  hidden
  border-left               5px solid lightGray
  borderBox()
  fl()

  a
    noTextDeco()
    width                   auto

.EmbedBoxLinkContent-title
  max-width                 100%
  display                   block
  font-size                 14px
  line-height               20px
  font-weight               600
  margin-bottom             8px
  color                     #636363 !important
  white-space               nowrap
  text-overflow             ellipsis
  overflow                  hidden

.EmbedBoxLinkContent-description
  display                   block
  height                    50px
  overflow                  hidden
  font-size                 14px
  color                     #3b3b3b !important
  line-height               16px

.EmbedBoxLinkContent-provider
  size                      auto auto
  color                     #acacac !important
  font-size                 12px
  line-height               16px
  abs                       0 0 0px auto
  display                   inline-block
  overflow                  visible

