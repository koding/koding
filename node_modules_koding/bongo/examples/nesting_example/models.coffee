{ Model } = require '../../lib'

class Insider extends Model
  @set
    schema    :
      name    :
        type  : String
        enum  : ['custom error message', ['aaa', 'bbb']]


class Outsider extends Model
  @set
    schema      :
      insider   : Insider
      insiders  : [Insider]

[@Outsider, @Insider] = [Outsider, Insider]
