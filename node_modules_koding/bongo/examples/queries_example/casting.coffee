{ Model } = require '../../lib'

class CastExample extends Model
  @set
    client          : 'localhost:27017/test'
    schema          :
      testFieldStr  : String
      testFieldNum  : Number

ceg = new CastExample
  testFieldStr  : '123'
  testFieldNum  : 123

ceg.save (err) ->
  throw err if err
  CastExample.one { _id: ceg._id + '' }, console.log
  CastExample.one { testFieldNum: '123' }, console.log
  CastExample.one { testFieldStr: 123 }, console.log
