{ User, Post } = require '../blog_example/models'

User.setClient 'localhost:27017/blog_example'

User.one ({ username: 'fooman' }), console.log

setTimeout ->

  User.some ({ username: { $ne: 'fooman' } }), ({ limit: 4 }), console.log

, 3000

setTimeout ->

  User.someData ({ username: 'fooman' }), ({ fullName: yes }), (err, cursor) ->
    cursor.nextObject console.log

, 6000

setTimeout ->

  Post.all {}, console.log

, 9000

