{ JsPath } = require '../../lib'

example =
  foo             :
    bar           :
      baz         : 10
    qux           : 11
    quux          :
      corge       :
        grapley   : 'test123'
      ham         : [ 'a', 'b', 'c' ]
      waldo       :
        fred      :
          spam    :
            ham   : 15
            eggs  : 16

console.log example

console.log JsPath.getAt      example, 'foo.bar.baz'                      # 10
console.log JsPath.setAt      example, 'foo.bar.baz', 12                  # [object Object] example
console.log JsPath.deleteAt   example, 'foo.quux.corge.grapley'           # true
console.log JsPath.getAt      example, 'foo.quux.waldo.fred.spam.ham'     # 15
console.log JsPath.deleteAt   example, 'foo.quux.waldo.fred.spam.eggs'    # true


console.log JsPath.deleteAt example, 'foo.quux.ham.0'

console.log JsPath.spliceAt example, 'foo.quux.ham', 0, 1

console.log JsPath.getAt example, 'foo.quux.ham'

console.log example
