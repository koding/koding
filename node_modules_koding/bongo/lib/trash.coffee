Model = require './model'

module.exports = class Trash extends Model

  @setSchema
    trashedAt      :
      type         : Date
      default      : -> new Date
    constructorName: String
    data           : Object

  @create:(instance, callback = ->) ->
    t = new this
      constructorName : instance.bongo_.constructorName
      data            : instance.get yes
    t.save (err) -> callback err
